    "babel-plugin-transform-class-constructor-call": "^6.0.14",
    "babel-plugin-transform-es2015-block-scoping": "^6.0.14",
    "babel-plugin-transform-export-extensions": "^6.0.14",
    "babel-plugin-transform-runtime": "^6.0.14",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.0.14",
    "babel-runtime": "^6.0.14",
    "css-loader": "^0.21.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "fontloader": "^1.2.2",
    "history": "^1.12.3",
    "node-libs-browser": "^0.5.3",
    "normalize.css": "^3.0.3",
    "picturefill": "^3.0.1",
    "postcss-custom-properties": "^5.0.0",
    "postcss-font-variant": "^2.0.0",
    "postcss-loader": "^0.7.0",
    "react": "^0.14.1",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "react-router": "^1.0.0-rc3",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "codeclimate-test-reporter": "github:smockle/javascript-test-reporter",
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.1",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.0",
    "sinon": "^1.17.1",
    "sinon-chai": "^2.8.0",
    "snyk": "^1.1.0"
  },
  "scripts": {
    "build": "NODE_ENV=production babel-node ./node_modules/.bin/webpack",
    "start": "NODE_ENV=development babel-node $(npm bin)/webpack-dev-server",
    "test": "mocha --require ./test/test-init --compilers js:babel-core/register --recursive && snyk test",
    "coverage": "$(npm bin)/istanbul cover $(npm bin)/_mocha -- --require ./test/test-init --compilers js:babel/register --recursive",
    "shrinkwrap": "rm -Rf node_modules && rm -f npm-shrinkwrap.json && npm i && npm shrinkwrap --dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/smockle-react.git"
