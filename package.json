    "coverage": "$(npm bin)/istanbul cover $(npm bin)/_mocha -- --require ./test/test-init --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smockle/smockle-react.git"
