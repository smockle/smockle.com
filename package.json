{
  "name": "smockle-react",
  "version": "1.0.0",
  "description": "Isomorphic app built with React.",
  "main": "app/main.js",
  "dependencies": {
    "autoprefixer": "6.0.3",
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "css-loader": "0.19.0",
    "extract-text-webpack-plugin": "0.8.2",
    "file-loader": "0.8.4",
    "fontloader": "1.2.2",
    "history": "1.12.0",
    "node-libs-browser": "0.5.3",
    "normalize.css": "3.0.3",
    "postcss-custom-properties": "5.0.0",
    "postcss-font-variant": "2.0.0",
    "postcss-loader": "0.6.0",
    "react": "v0.14.0-rc1",
    "react-dom": "0.14.0-rc1",
    "react-hot-loader": "1.3.0",
    "react-router": "1.0.0-rc1",
    "style-loader": "0.12.4",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.0"
  },
  "devDependencies": {
    "chai": "3.3.0",
    "codeclimate-test-reporter": "0.1.1",
    "coveralls": "2.11.4",
    "istanbul": "https://github.com/gotwarlost/istanbul.git#master",
    "jsdom": "6.5.1",
    "mocha": "2.3.3",
    "react-addons-test-utils": "0.14.0-rc1",
    "sinon": "1.17.1",
    "sinon-chai": "2.8.0"
  },
  "scripts": {
    "build": "NODE_ENV=production babel-node $(npm bin)/webpack",
